{
  "name": "demo-nuxt-bridge",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "nuxi preview",
    "lint:js": "eslint --ext \".js,.ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js -- --fix",
    "reinstall": "rimraf pnpm-lock.yaml && pnpm clean && pnpm i",
    "clean": "pnpm /^clean:/",
    "clean:nuxt": "rimraf .nuxt",
    "clean:node_modules": "rimraf node_modules"
  },
  "devDependencies": {
    "@nuxt/bridge": "npm:@nuxt/bridge-edge@3.0.0-28177393.984ce7c",
    "@pinia/nuxt": "^0.4.11",
    "@unocss/nuxt": "^0.53.5",
    "@vueuse/nuxt": "^10.2.1",
    "nuxt-edge": "latest",
    "pinia": "^2.1.4"
  },
  "dependencies": {
    "vue-client-only": "^2.1.0",
    "vue-meta": "^2.4.0",
    "vue-no-ssr": "^1.1.1",
    "vue-router": "^3.6.5",
    "@nuxt/ui-templates": "^1.3.1",
    "unocss": "^0.54.0"
  }
}
